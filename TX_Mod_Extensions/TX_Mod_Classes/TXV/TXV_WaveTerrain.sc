	extractLayerParameterSuffixes {
		var tempArray;
		// extract suffixes from Layer A parameter names
		tempArray = dictCurrentParameterVals.keys.asArray.select({arg item; item.asString.split.at(2).keep(7) == "layerA_"});
		classData.arrLayerParameterSuffixes = tempArray.collect({arg item;
			// remove "/TXWaveTerrain_1/layerA_" from front
			var holdString = item.asString.keep((item.asString.size-24).neg);
			// testing xxx
			//[item, holdString].postln;
			holdString;
		});
	}

	saveWTLayerPresetsToFile {
		var holdFile, holdFileData, holdPath, holdPath2;
		holdPath = PathName.new(Platform.userAppSupportDir +/+ "TXModular/TXVWaveTerrainLayerPresets.tx");
		holdFileData = [
			"TX Modular System TXV_WaveTerrain Layer Preset Data",
			classData.wtLayerPresetData.getPairs,
		];
		holdFile = File(holdPath.fullPath, "w");
		if (File.exists(holdPath.fullPath)) {
			holdFile.write("#" ++  holdFileData.asCompileString ++ "\n");
			//"Saving TXVWaveTerrainLayerPresets.tx".postln;
		} {
			"".postln;
			"TX Warning: saveWTLayerPresetsToFile: File doesn't exist: ".postln;
			holdPath.postln;
		};
		holdFile.close;
	}

	loadWTLayerPresetsFromFile {
		var holdPath, holdFile, holdFileData;
		holdPath = PathName.new(Platform.userAppSupportDir +/+ "TXModular");
		holdFile = PathName.new(Platform.userAppSupportDir +/+ "TXModular" +/+ "TXVWaveTerrainLayerPresets.tx");
		// if directory doesn't exist, create it.
		if (holdPath.isFolder.not, {
			// NEW CODE
			holdPath.fullPath.makeDir;
		});
		if (File.exists(holdFile.fullPath),  {
			// if file exists, update values.
			holdFileData = thisProcess.interpreter.executeFile(holdFile.fullPath);
			if (holdFileData.class == Array, {
				classData.wtLayerPresetData.putPairs(holdFileData[1]);
				//"Loading TXVWaveTerrainLayerPresets.tx".postln;
			});
		});
	}

