	copyLookupCurve4ParmsToColorMix {
		var arrSuffixes = [];
		// copy lookupCurve4 parameters to colorMixCurve parameters
		classData.arrColorParameterSuffixes.do({arg suffix;
			if (suffix.keep(13) == "colorMixCurve", {
				arrSuffixes = arrSuffixes.add(suffix);
			});
		});
		arrSuffixes.do({arg suffix, i;
			var copyParamName = "/TXWaveTerrain_1/lookupCurve4" ++ suffix.keep(13 -suffix.size);
			var	paramName = "/TXWaveTerrain_1/" ++ suffix;
			var paramVal = dictCurrentParameterVals[copyParamName.asSymbol];
			dictCurrentParameterVals[paramName.asSymbol] = paramVal;
			this.sendModuleParmToTXV(paramName, paramVal);
		});
	}

	// layer presets

	loadPresetToLayer {arg presetName, layerPrefix, excludeModulationData = false;  // e.g. ("circles", "layerA_", true)
		var layerData, paramName, paramVal;
		//("Loading Wave Terrain Layer Preset " ++ presetName).postln;
		if (classData.wtLayerPresetData[presetName.asSymbol].notNil, {
			layerData = ();
			layerData.putPairs(classData.wtLayerPresetData[presetName.asSymbol]);
			// remove modulation data
			if (excludeModulationData == true, {
				layerData.keysDo({arg key;
					var last = key.asString.split.last;
					if ( (last == "fixedModMix") or: (last == "useExtMod") or: (last == "extModValue"), {
						layerData[key] = nil;
					});
				});
			});
			classData.arrLayerParameterSuffixes.do({arg suffix;
				paramVal = layerData[suffix.asSymbol];
				if (paramVal.notNil, {
					paramName = "/TXWaveTerrain_1/" ++ layerPrefix ++ suffix;
					dictCurrentParameterVals[paramName.asSymbol] = paramVal;
					this.sendModuleParmToTXV(paramName, paramVal);
				});
			});
			this.setSynthArgSpec("i_layerA_presetText", presetName);
			this.buildGuiSpecArray;
			system.showView;
		});
	}

	savePresetFromLayer {arg presetName, layerPrefix;  // e.g.  ("circles", "layerA_")
		var layerData = ();
		var paramName;
		//("Saving Wave Terrain Layer Preset Name: " ++ presetName).postln;
		if (presetName != "", {
			classData.arrLayerParameterSuffixes.do({arg suffix;
				paramName = "/TXWaveTerrain_1/" ++ layerPrefix ++ suffix;
				layerData[suffix.asSymbol] = dictCurrentParameterVals[paramName.asSymbol];
